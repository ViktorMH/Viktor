<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.usuario.comidaapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/RestaurantApp"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CaldoActivity"
            android:label="Caldo" />
        <activity
            android:name=".SegundoActivity"
            android:label="Segundo" />
        <activity
            android:name=".CombinadoActivity"
            android:label="Combinado" />
        <activity
            android:name=".PedidoActivity"
            android:label="Pedido" />
        <activity
            android:name=".MenuDiaActivity"
            android:label="Menú" />
        <activity
            android:name=".ReportesActivity"
            android:label="Reportes" />
        <activity android:name=".CalculoActivity"
            android:label="Cálculo de ganancias">
        </activity>
    </application>

</manifest>